function sumArrayElements(arr) {
    // Convert each element of the array to a number and sum them up
    const sum = arr.map(Number).reduce((acc, curr) => acc + curr, 0);

    // Print the sum as a single integer
    console.log(sum);
}

// Sample Input
const inputArray = "1 2 3 4 10 11".split(" ").map(Number);

// Call the function with the input array
sumArrayElements(inputArray);

